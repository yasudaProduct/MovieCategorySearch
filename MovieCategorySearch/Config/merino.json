      {
        "AssemblyName": "MovieCategorySearch.Application",
        "EndMatchNames": [
          "Service"
        ]
      },
      {
        "AssemblyName": "MovieCategorySearch.Infrastructure",
        "EndMatchNames": [
          "Repository",
          "QueryService",
          "Client"
        ],
        "ExactMatchNames": [
          "InMemoryUserRepository"
        ]
      }
    ],
